generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Stats {
  id           Int      @id @default(autoincrement())
  visitors     Int
  pageviews    Int
  avg_duration Int
  bounce_rate  Float
  created_at   DateTime @default(now())
} 